# Exemplo de arquivo .env
# Copie este arquivo para .env e preencha com suas chaves

# API Keys
ANTHROPIC_API_KEY=sua_chave_anthropic_aqui
GOOGLE_API_KEY=sua_chave_google_aqui

# Supabase Configuration
SUPABASE_URL=https://seu-projeto.supabase.co
SUPABASE_KEY=sua_chave_publica_aqui
SUPABASE_SERVICE_KEY=sua_service_role_key_aqui

# Database Provider
DATABASE_PROVIDER=supabase

# Whisper Model (tiny, base, small, medium, large)
WHISPER_MODEL=base

# Transcription Provider (openai_whisper, google_gemini)
TRANSCRIPTION_PROVIDER=openai_whisper

# LLM Provider (anthropic_claude, google_gemini)
LLM_PROVIDER=anthropic_claude
